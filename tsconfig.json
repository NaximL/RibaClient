{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@api/*": ["./api/*"],
      "@components/*": ["./components/*"],
      "@screens/*": ["./screens/*"],
      "@assets/*": ["./assets/*"],
      "@emoji/*": ["./assets/emoji/*"],
      "@img/*": ["./assets/image/*"],
      "@store/*": ["./store/*"],
      "@config/*": ["./config/*"],
      "@styles/*": ["./styles/*"]
    }
  }
}